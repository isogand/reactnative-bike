{
  "name": "react-native-fashion",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint --ext .ts, .tsx . --max-warnings 0",
    "tsc": "tsc",
    "ci": "yarn lint && yarn tsc"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@freakycoder/react-native-bounceable": "^0.2.5",
    "@freakycoder/react-native-multiple-select": "^0.3.0",
    "@mindinventory/react-native-tab-bar-interaction": "^2.2.1",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@shopify/restyle": "^2.0.0",
    "@types/d3-shape": "^3.0.2",
    "axios": "^0.26.1",
    "expo": "~44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-constants": "~13.0.1",
    "expo-font": "~10.0.4",
    "expo-image-picker": "~12.0.1",
    "expo-linear-gradient": "~11.0.3",
    "expo-status-bar": "~1.2.0",
    "formik": "^2.2.9",
    "lottie-react-native": "5.0.1",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-spinkit": "^1.5.2",
    "react-native-bouncy-checkbox": "^2.1.10",
    "react-native-deck-swiper": "^2.0.5",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~2.3.1",
    "react-native-redash": "^16.2.3",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.2.1",
    "react-native-screens": "~3.10.1",
    "react-native-shared-element": "^0.8.4",
    "react-native-stepper-ui": "^0.0.9",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "rn-fetch-blob": "^0.12.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-react-native-wcandillon": "^3.7.2",
    "eslint-config-universe": "^7.0.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "prettier": "^2.3.2",
    "typescript": "~4.3.5"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "@typescript-eslint/ban-types": "warn",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-empty-function": "warn",
      "no-unused-vars": "off",
      "no-empty": "warn",
      "react/prop-types": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn"
      ]
    },
    "root": true,
    "parser": "@typescript-eslint/parser",
    "ignorePatterns": [
      "node_modules"
    ],
    "globals": {
      "guestId": true,
      "userId": true,
      "LUMEN_DOT_ENV": true
    },
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    }
  },
  "prettier": {
    "tabWidth": 4,
    "semi": true,
    "bracketSpacing": false
  },
  "private": "true"
}
